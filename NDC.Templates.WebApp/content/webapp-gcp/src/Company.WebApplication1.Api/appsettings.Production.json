{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "ConnectionStrings": {
#if (HasDatabase)
    "Database": "Host=${DATABASE_HOST};Database=${DATABASE_NAME};Username=${DATABASE_USER};Password=${DATABASE_PASSWORD};Port=5432;SSL Mode=Require",
#endif
#if (IncludeCache)
    "Redis": "${REDIS_CONNECTION_STRING}",
#endif
    "DefaultConnection": "${DATABASE_CONNECTION_STRING}"
  },
#if (IncludeStorage)
  "GoogleCloudStorage": {
    "BucketName": "${GCS_BUCKET_NAME}",
    "ProjectId": "${GOOGLE_PROJECT_ID}"
  },
#endif
#if (IncludeMail)
  "SendGrid": {
    "ApiKey": "${SENDGRID_API_KEY}",
    "FromEmail": "${FROM_EMAIL}",
    "FromName": "${FROM_NAME}"
  },
#endif
#if (IncludeQueue)
  "GoogleCloudPubSub": {
    "ProjectId": "${GOOGLE_PROJECT_ID}",
    "TopicId": "${PUBSUB_TOPIC_ID}",
    "SubscriptionId": "${PUBSUB_SUBSCRIPTION_ID}"
  },
#endif
  "ASPNETCORE_ENVIRONMENT": "Production"
}