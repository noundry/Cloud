{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "DatabaseProvider": "PostgreSQL",
  "CacheProvider": "Redis",
  "StorageProvider": "GoogleCloudStorage",
  "MailProvider": "SendGrid",
  "QueueProvider": "GoogleCloudPubSub",
  "ConnectionStrings": {
#if (HasDatabase)
    "Database": "Host=localhost;Database=Company_WebApplication1_db;Username=postgres;Password=postgres;Port=5432",
#endif
#if (IncludeCache)
    "Redis": "localhost:6379",
#endif
#if (IncludeQueue)
    "PubSub": "projects/your-project-id/topics/your-topic",
#endif
    "DefaultConnection": ""
  },
#if (IncludeStorage)
  "GoogleCloudStorage": {
    "BucketName": "your-bucket-name",
    "ProjectId": "your-project-id"
  },
#endif
#if (IncludeMail)
  "SendGrid": {
    "ApiKey": "your-sendgrid-api-key",
    "FromEmail": "noreply@example.com",
    "FromName": "Company WebApplication1"
  },
#endif
#if (IncludeQueue)
  "GoogleCloudPubSub": {
    "ProjectId": "your-project-id",
    "TopicId": "your-topic-id",
    "SubscriptionId": "your-subscription-id"
  },
#endif
  "HealthChecks": {
    "UI": {
      "ApiPath": "/health",
      "UIPath": "/healthchecks-ui"
    }
  }
}