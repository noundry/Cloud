{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "DatabaseProvider": "PostgreSQL",
  "CacheProvider": "Redis", 
  "StorageProvider": "FileSystem",
  "MailProvider": "SMTP",
  "QueueProvider": "InMemory",
  "ConnectionStrings": {
#if (HasDatabase)
    "Database": "Host=localhost;Database=Company_WebApplication1_db;Username=postgres;Password=postgres;Port=5432",
#endif
#if (IncludeCache)
    "Redis": "localhost:6379",
#endif
    "DefaultConnection": ""
  },
#if (IncludeStorage)
  "FileSystemStorage": {
    "BasePath": "/app/storage"
  },
#endif
#if (IncludeMail)
  "SmtpSettings": {
    "Host": "smtp.gmail.com",
    "Port": 587,
    "EnableSsl": true,
    "Username": "your-email@gmail.com",
    "Password": "your-password",
    "FromEmail": "noreply@example.com",
    "FromName": "Company WebApplication1"
  },
#endif
  "HealthChecks": {
    "UI": {
      "ApiPath": "/health",
      "UIPath": "/healthchecks-ui"
    }
  }
}