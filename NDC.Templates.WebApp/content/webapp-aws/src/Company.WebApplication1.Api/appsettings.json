{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
#if (IncludeJobs)
      ,"Hangfire": "Information"
#endif
    }
  },
  "AllowedHosts": "*",
  
  "ConnectionStrings": {
#if (HasDatabase)
#if (UsePostgreSQL)
    "Database": "Host=localhost;Port=5432;Database=Company.WebApplication1;Username=postgres;Password=postgres",
#endif
#if (UseMySQL)
    "Database": "Server=localhost;Port=3306;Database=Company.WebApplication1;Uid=root;Pwd=mysql;",
#endif
#if (UseSqlServer)
    "Database": "Server=localhost,1433;Database=Company.WebApplication1;User Id=sa;Password=YourStrong@Passw0rd;TrustServerCertificate=true;",
#endif
#endif
#if (IncludeCache)
    "Redis": "localhost:6379",
#endif
#if (IncludeQueue)
    "RabbitMQ": "amqp://guest:guest@localhost:5672/"
#endif
  },
  
  "Services": {
#if (HasDatabase)
    "Database": {
#if (UsePostgreSQL)
      "Type": "PostgreSQL",
      "Provider": "Npgsql"
#endif
#if (UseMySQL)
      "Type": "MySQL",
      "Provider": "Pomelo"
#endif
#if (UseSqlServer)
      "Type": "SqlServer", 
      "Provider": "Microsoft"
#endif
    },
#endif
#if (IncludeCache)
    "Cache": {
      "Type": "Redis",
      "Enabled": true,
      "DefaultExpiration": "01:00:00"
    },
#endif
#if (IncludeStorage)
    "Storage": {
      "Type": "S3",
      "Enabled": true,
      "BucketName": "Company.WebApplication1-storage"
    },
#endif
#if (IncludeMail)
    "Email": {
      "Type": "SMTP",
      "Provider": "Local",
      "From": "noreply@Company.WebApplication1.com",
      "FromName": "Company.WebApplication1"
    },
#endif
#if (IncludeQueue)
    "MessageQueue": {
      "Type": "RabbitMQ",
      "Enabled": true
    },
#endif
#if (IncludeJobs)
    "Jobs": {
      "Type": "Hangfire",
      "Enabled": true,
      "DashboardEnabled": true
    }
#endif
  },

#if (IncludeStorage)
  "S3": {
    "BucketName": "Company.WebApplication1-storage",
    "Region": "us-east-1",
    "ServiceUrl": "http://localhost:9000",
    "AccessKey": "minioadmin",
    "SecretKey": "minioadmin",
    "ForcePathStyle": true
  },
#endif

#if (IncludeMail)
  "Email": {
    "Smtp": {
      "Host": "localhost",
      "Port": 1025,
      "EnableSsl": false,
      "Username": "",
      "Password": "",
      "From": "noreply@Company.WebApplication1.com",
      "FromName": "Company.WebApplication1"
    }
  }
#endif
}